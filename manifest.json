{
  "name": "No Guys Allowed",
  "description": "Replace the word 'guys' with a gender neutral and inclusive term.",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Emma Koza",
  "browser_action": {
    "default_title": "No Guys Allowed"
  },
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "content.js"
    ],
    "run_at": "document_end"
  }]
}

